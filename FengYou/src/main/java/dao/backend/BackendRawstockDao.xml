<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.backend.BackendRawstockDao">
   <insert id="addawstock" parameterType="Rawstock" >
  INSERT INTO rawstock(id,hotelId,houseId,store) VALUES(null,#{hotelId},#{houseId},#{store})
   </insert>
   
   	
	   <select id="getlist" resultType="Rawstock" >
	select * from Rawstock  where houseId=#{houseId}
	</select>
	
		<!-- 修改房间信息 -->
	   <update id="updateraw" parameterType="Rawstock" >
  update Rawstock set
		store=#{store}
		where houseId=#{houseId}
   </update>
</mapper>